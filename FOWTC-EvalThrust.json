{
    "Actions": {
        "load data": true,
        "load simulation": false,
        "generate model": true,
        "run statics": true,
        "run modal": false,
        "run dynamics": true,
        "postprocess results": true,
        "plot results": true,
        "batch simulations": true
    },
    "File IO": {
        "input": {
            "Orcaflex data": "FOWTC-EvalThrust.yml"
        },
        "output": {
            "Orcaflex sim": "FOWTC-EvalThrust-last.sim",
            "Orcaflex data": "FOWTC-EvalThrust-last.yml"
        }
    },
    "Batch": {
        "save all": { "Orcaflex simulations": true, "Orcaflex data": true },
        "thrust curve": {
            "wind speed": { "from": 3, "to": 25, "step": 0.5 },
            "curves": { "Ct": true, "Cp": true, "Lz force": true }
        }
    },
    "Analysis": {
        "statics enabled" : true,
        "stage duration": [ 1000.0, 600.0 ],
        "statics": {
            "max iterations": 400,
            "tolerance": 1.0e-6,
            "damping": [1.0, 10.0]
        },
        "dynamics": {
            "method": "implicit",
            "variable time step": false,
            "time step": 0.05, 
            "max iterations": 100, 
            "tolerance": 25.0e-6, 
            "log precision": "single",
            "sample": 0.1
        }
    },
    "PostProcessing": {
        "period": { "stage": 2 },

        "plots": {
            "thrust curve": {
                "same fig": true,
                "group": "same column"
            }
        },

        "export format": { 
            "batch": ["excel", "csv"]
        } 
    },
    "Save options": {
        "Orcaflex data": true,
        "Orcaflex simulation": true,
        "results": true
    }
}